{
  "expo": {
    "name": "eSAnjeevaniApp",
    "slug": "eSAnjeevaniApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-2x.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/eSanjevani_small.png",
      "resizeMode": "contain",
      "backgroundColor": "#34bccc"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "notification": {
      "icon": "./assets/images/logo-2x.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo-2x.png",
          "color": "#ffffff"
        }
      ]
    ]
  }
}
